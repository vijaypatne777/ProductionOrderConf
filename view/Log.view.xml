<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="Prod_Ord_Conf.Production_Ord_Conf.controller.Log" xmlns:html="http://www.w3.org/1999/xhtml"
	id="vLog">
	<Page navButtonPress="onNavBack"
	      showNavButton="true">
	<Table id="msgTable" items="{ path: '/msgs'}" growing="true">
		<headerToolbar>
			<Toolbar>
				<Title text="Log- Upload Prodction Order Confirmation" level="H2"/>
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column width="1rem">
				<Text text="Message Type"/>
			</Column>
			<Column width="12rem">
				<Text text="Message"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{Mtype}"/>
					<Text text="{Msg}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
	</Page>
</mvc:View>